{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/components/master-components/CreateUser.jsx", "app/components/adminComponents/dashboad/Analytics.jsx", "app/components/adminComponents/dashboad/data.js", "app/components/adminComponents/dashboad/ExpensesAnalysis.tsx", "app/components/adminComponents/dashboad/Revenue.jsx", "app/components/adminComponents/dashboad/dataSummary.js", "app/components/nav-main.tsx", "app/components/app-sidebar.jsx", "app/components/adminComponents/employee/Image.jsx", "app/components/login-form.jsx", "app/login/[...nextauth].js.tsx"],
  "exclude": ["node_modules"]
}
